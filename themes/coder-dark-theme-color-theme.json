{
  "name": "bhupeshb7-coder-dark-theme",
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    // Base UI colors
    "editor.background": "#0e0e12",
    "editor.foreground": "#e2e2e9",
    "sideBar.background": "#080808",
    "activityBar.background": "#080808",
    "statusBar.background": "#080808",
    "titleBar.activeBackground": "#080808",

    // Accent colors
    "activityBar.foreground": "#f9ffb9", 
    "activityBarBadge.background": "#000000",
    "activityBar.inactiveForeground": "#6d6868",
    "activityBarBadge.foreground": "#ffeb7b",
    "statusBar.foreground": "#f9ffb9",
    "titleBar.activeForeground": "#f9ffb9",

    // Borders and separators
    "activityBar.border": "#1a1a2260",
    "sideBar.border": "#1a1a2260",
    "tab.border": "#1a1a2260",

    // Editor highlights
    "editor.lineHighlightBackground": "#00000090",
    "editor.selectionBackground": "#b71cff99",
    "editor.selectionHighlightBackground": "#3b3b5a40",
    "editor.selectionHighlightBorder": "#7676cb80",

    // Cursor and line numbers
    "editorCursor.foreground": "#f9ffb9",
    "editorLineNumber.foreground": "#6c6c8c",
    "editorLineNumber.activeForeground": "#b8ff65",

    // Lists and selections
    "list.activeSelectionForeground": "#ffffff",
    "list.focusBackground": "#1a1a2c",
    "list.focusForeground": "#6eff99",
    "list.highlightForeground": "#6eff99",
    "list.hoverBackground": "#1a1a2c",
    "list.hoverForeground": "#f9ffb9",
    "list.inactiveSelectionBackground": "#1a1a2c50",
    "list.inactiveSelectionForeground": "#e2e2e9",

    // Tabs
    "tab.activeBackground": "#10101cba",
    "tab.activeBorder": "#f9ffb9",
    "tab.activeForeground": "#f9ffb9",
    "tab.inactiveBackground": "#090910f4",
    "tab.inactiveForeground": "#b8b8c8",

    // Notifications
    "notifications.background": "#0e0e12",
    "notifications.border": "#f9ffb940",
    "notifications.foreground": "#e2e2e9",
    "notificationCenterHeader.background": "#0e0e12",
    "notificationCenterHeader.foreground": "#f9ffb9",

    // Input and buttons
    "input.background": "#14141e",
    "input.foreground": "#e2e2e9",
    "button.background": "#f9ffb950",
    "button.foreground": "#ffffff",

    // Suggestion widget
    "editorSuggestWidget.background": "#14141e",
    "editorSuggestWidget.border": "#282840",
    "editorSuggestWidget.foreground": "#e2e2e9",
    "editorSuggestWidget.highlightForeground": "#f9ffb9",
    "editorSuggestWidget.selectedBackground": "#282840",

    // Sidebar
    "sideBarSectionHeader.background": "#090910",
    "sideBarSectionHeader.foreground": "#e2e2e9",

    // Panels
    "panel.background": "#090910",
    "panel.border": "#1a1a2260",
    "panelTitle.activeForeground": "#f9ffb9",
    "panelTitle.inactiveForeground": "#b8b8c8",

    // Peek view
    "peekView.border": "#f9ffb950",
    "peekViewEditor.background": "#0e0e12",
    "peekViewEditor.matchHighlightBackground": "#3b3b5a40",
    "peekViewResult.background": "#0e0e12",
    "peekViewResult.fileForeground": "#f9ffb9",
    "peekViewResult.lineForeground": "#e2e2e9",
    "peekViewTitle.background": "#0e0e12",
    "peekViewTitleLabel.foreground": "#f9ffb9",

    // ðŸ”¹ Quick Open / Command Palette
    "quickInput.background": "#121212",
    "quickInput.foreground": "#9e9e9e",
    "quickInputTitle.background": "#0a0a0a",
    "quickInputList.focusBackground": "#1e1e1e",
    "quickInputList.focusForeground": "#f9ffb9",

    // ðŸ”¹ Command Palette dropdown
    "pickerGroup.foreground": "#f9ffb9",
    "pickerGroup.border": "#1e1e1e",

    // ðŸ”¹ Context Menu
    "menu.background": "#111111",
    "menu.foreground": "#dddddd",
    "menu.selectionBackground": "#1e1e1e",
    "menu.selectionForeground": "#f9ffb9",
    "menu.separatorBackground": "#2a2a2a",

    // ðŸ”¹ Dropdowns and Lists
    "dropdown.background": "#111111",
    "dropdown.foreground": "#ffffff",
    "dropdown.border": "#2a2a2a",

    // ðŸ”¹ Borders and highlights
    "focusBorder": "#f9ffb9",
    "widget.shadow": "#00000080",
    "selection.background": "#f9ffb940",
    // Terminal
    "terminalCursor.foreground": "#f9ffb9",

    // Tree and guides
    "tree.indentGuidesStroke": "#282840"
  },
  "tokenColors": [
    // Keywords, storage types and modifiers
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff9d60",
        "fontStyle": "italic"
      }
    },

    // Control flow keywords
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#ff9d60",
        "fontStyle": "bold"
      }
    },

    // Variables, properties, placeholders
    {
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#def7ff"
      }
    },

    // Functions
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#ffdf5d"
      }
    },

    // Object properties, parameters
    {
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "variable.parameter",
        "variable.parameter.function.language.special"
      ],
      "settings": {
        "foreground": "#b5ff9d"
      }
    },

    // Strings
    {
      "scope": [
        "string",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.template",
        "string.template"
      ],
      "settings": {
        "foreground": "#00ff95"
      }
    },

    // Numbers, constants
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character",
        "constant.escape",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#ff7d7d"
      }
    },

    // HTML/JSX Tags
    {
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag",
        "meta.tag.sgml"
      ],
      "settings": {
        "foreground": "#f9ffb9"
      }
    },

    // HTML Attributes
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ffee97"
      }
    },

    // CSS Properties
    {
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#82e2ff"
      }
    },

    // CSS Values
    {
      "scope": [
        "source.css support.constant",
        "source.sass support.constant",
        "source.scss support.constant",
        "source.less support.constant",
        "source.stylus support.constant",
        "source.postcss support.constant"
      ],
      "settings": {
        "foreground": "#b5ff9d"
      }
    },

    // CSS Selectors
    {
      "scope": [
        "entity.other.attribute-name.id",
        "entity.other.attribute-name.class",
        "entity.other.attribute-name.pseudo-class",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#ffee97"
      }
    },

    // Comments
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6d7180",
        "fontStyle": "italic"
      }
    },

    // JSON Property Names
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#82e2ff"
      }
    },

    // JSON Values
    {
      "scope": [
        "support.type.property-value.json",
        "string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#e6ff95"
      }
    },

    // Markdown Headings
    {
      "scope": [
        "markup.heading",
        "entity.name.section.markdown",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#f9ffb9",
        "fontStyle": "bold"
      }
    },

    // Markdown Links
    {
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#82e2ff"
      }
    },

    // Punctuation
    {
      "scope": [
        "punctuation",
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#b8b8c8"
      }
    },

    // Classes, types, interfaces
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.scope-resolution",
        "entity.name.interface",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#ffee97"
      }
    },

    // Regular expressions
    {
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": {
        "foreground": "#ff7d7d"
      }
    },
    // for other objects
    {
      "scope": ["variable.other.object.js", "string.other.link.title.markdown"],
      "settings": {
        "foreground": "#f9ffb9"
      }
    }
  ]
}
