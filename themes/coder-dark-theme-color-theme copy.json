        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#FF00C2",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strike"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#05DDBB50"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#FF0A00"
      }
    },
    {
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#FF0A00"
      }
    },
    {
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#FF0A00"
      }
    },
    {
      "scope": [
        "punctuation.definition.raw.markdown",
        "punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#FF00B0"
      }
    },
    {
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#05DDBB"
      }
    },
    {
      "scope": ["meta.separator"],
      "settings": {
        "foreground": "#A7A8AF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#05DDBB"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#FF0A00"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#FF293B"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#a3edf8"
      }
    }
